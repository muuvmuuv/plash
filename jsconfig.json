{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "checkJs": true,
    "jsx": "preserve",
    "types": ["vite/client"],
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "lib": ["ESNext", "DOM"]
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
